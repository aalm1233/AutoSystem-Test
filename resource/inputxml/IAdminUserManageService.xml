					</element>
					<element type="String" attribute="Seller.forbid" level="1"
						name="forbid" location="false"></element>
					<element type="String" attribute="Seller.company" level="1"
						name="company" location="Seller.company"></element>
					<element type="String" attribute="Seller.businessLicense"
